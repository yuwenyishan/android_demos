<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jax.demos">

    <application
        android:allowBackup="true"
        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".constraintlayout.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="demos"
                    android:path="/constraintLayout"
                    android:scheme="jax" />
            </intent-filter>
        </activity>
        <activity
            android:name=".constraintlayout.ConstraintAnimator"
            android:label="constraintAnimator" />
        <activity
            android:name=".constraintlayout.DoubleLayoutTransition"
            android:label="DoubleLayoutTransition" />
    </application>

</manifest>
